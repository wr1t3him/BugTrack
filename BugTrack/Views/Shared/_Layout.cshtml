        <div class="container-fluid">
            <div class="row row-offcanvas row-offcanvas-right">
                <!-- partial:partials/_sidebar.html -->
                <nav class="bg-white sidebar sidebar-offcanvas" id="sidebar">
                    <div class="user-info">
                        <img src="images/face.jpg" alt="">
                        <p class="name">Richard V.Welsh</p>
                        <p class="designation">Manager</p>
                        <span class="online"></span>
                    </div>
                    <ul class="nav">
                        <li class="nav-item">
                            @Html.Partial("_LoginPartial")
                            <a class="nav-link" href="">
                                <img src="images/icons/5.png" alt="">
                                <span class="menu-title"></span>
                            </a>
                        </li>
                        <li class="nav-item active">
                            <a class="nav-link" href=@Url.Action("Index", "Home")>
                                <img src="images/icons/1.png" alt="">
                                <span class="menu-title">Home</span>
                            </a>
                        </li>
                        @if (User.IsInRole("Admin") || (User.IsInRole("ProjectManager")))
                        {
                            <li class="nav-item">
                                <a class="nav-link" href=@Url.Action("Index", "User")>
                                    <img src="images/icons/2.png" alt="">
                                    <span class="menu-title">Employees</span>
                                </a>
                            </li>
                        }
                        <li class="nav-item">
                            <a class="nav-link" href=@Url.Action("Index", "Projects")>
                                <img src="images/icons/005-forms.png" alt="">
                                <span class="menu-title">Projects</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href=@Url.Action("Index", "Tickets")>
                                <img src="images/icons/4.png" alt="">
                                <span class="menu-title">Tickets</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href=@Url.Action("Index" , "TicketComments")>
                                <img src="images/icons/6.png" alt="">
                                <span class="menu-title">Ticket Comments</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href=@Url.Action("Index", "TicketAttachments")>
                                <img src="images/icons/7.png" alt="">
                                <span class="menu-title">Attachments</span>
                            </a>
                        </li>
